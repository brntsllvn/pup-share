<div class='col-md-4 walk panel panel-default'>
  <div class="col-md-5">
    <p><%= "#{phone_number.phone_type}" %></p>
  </div>

  <div class="col-md-5">
    <p><%= "#{phone_number.number}" %></p>   
  </div>

  <div class="col-md-2">
    <%= link_to user_phone_number_path(current_user, phone_number), method: :delete, data: { confirm: "Are you sure? Any walks you've created with this phone number will also be destroyed." }, class: 'btn btn-danger' do %>
    <span class="glyphicon glyphicon-remove-sign" aria-hidden="true"></span>
    <% end %>
  </div>
</div>
<div class="col-md-12 text-center">
  <h1>Upcoming Walks</h1> 
  <%= link_to 'Past Walks', user_past_walks_path(current_user), class:'btn btn-primary' %>
</div>

<br>

<div class="col-md-12 text-center">
  <% @walks_plus_offers.each_with_index do |walk, index| %>
  <%= render partial: 'walks/walk', locals: { walk: walk, index: index } %>
  <% end %>
</div>
<h1>Me</h1>

<%= render @user %>
<br>

<h1>My Pups</h1> 
<%= link_to 'New Pup', new_user_pup_path(current_user), class: 'btn btn-primary' %>
<br>
<br>
<% @pups.each do |pup| %>
<%= render partial: 'pups/pup', locals: { pup: pup } %>
<br>
<% end %>

<h1>Upcoming Walks</h1> 
<%= link_to 'New Job', new_job_path, class: 'btn btn-primary' %>
<br>
<br>
<% @jobs.each do |job| %>
<%= render partial: 'jobs/job', locals: { job: job } %>
<% end %>

